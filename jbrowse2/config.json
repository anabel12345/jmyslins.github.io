{
  "configuration": {
    "containerId": "jbrowse",
    "tracks": {
      "track1": {
        "store": {
          "url": "your_track_data_file.json",
          "type": "JBrowse/Store/SeqFeature/NCList"
        },
        "type": "JBrowse/View/Track/CanvasFeatures",
        "label": "Track 1",
        "key": "Track 1",
        "assemblyNames": ["your_assembly_name"],
        "category": "Your Category",
        "maxFeatureScreenDensity": 1,
        "storeClass": "JBrowse/Store/SeqFeature/NCList",
        "renderer": {
          "type": "JBrowse/View/FeatureGlyph/Box",
          "displayMode": "compact",
          "style": {
            "className": "feature"
          }
        },
        "compress": 0,
        "description": "Description of your track"
      }
    },
    "defaultSession": {
      "name": "Default Session",
      "view": {
        "id": "LinearGenomeView",
        "tracks": [
          {
            "trackId": "track1",
            "type": "CanvasFeatures",
            "name": "Track 1",
            "assemblyNames": ["your_assembly_name"]
          }
        ]
      },
      "location": {
        "refName": "your_reference_sequence",
        "start": 1,
        "end": 1000000
      },
      "tracks": ["track1"]
    }
  },
  "plugins": ["@gmod/jbrowse-web"],
  "sequenceAdapter": {
    "type": "JBrowse/Model/SequenceAdapter/CramAdapter"
  },
  "defaultSessionTimeout": 1800000,
  "trackSelector": {
    "maxFilesToOpen": 1000
  }
}
